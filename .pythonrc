def reload(func):
    """
    function reload tool

    :type func: function
    """
    import sys
    import importlib

    _mo = importlib.reload(sys.modules.get(func.__module__))
    ret = eval('_mo.{}'.format(func.__qualname__))

    del _mo
    del sys
    del importlib
    return ret
